def countConnected(head, arr: List[int], m: int) -> int:
    i = 0  
    f,ct = 0,0
    node = head
    d = {} 
    for i in range(m) :
        d[arr[i]] = i  
    while node != None and i < m :
        if node.data in d.keys() : 
            arr[d[node.data]] = -1
            f = 1 
        else :
            if f :
                ct += 1 
            f = 0 
        node = node.next 
    if f :
        ct += 1 
    return ct
